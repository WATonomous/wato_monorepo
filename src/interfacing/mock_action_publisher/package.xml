<?xml version="1.0"?>
<package format="3">
  <name>mock_action_publisher</name>
  <version>0.0.1</version>
  <description>Mock node publishing /action/ackermann and /action/is_idle with a SetBool service to toggle idle.</description>

  <maintainer email="shawn.yang@uwaterloo.ca">bolty</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>ackermann_msgs</depend>
  <depend>std_msgs</depend>
  <depend>std_srvs</depend>

  <export>
    <build_type>ament_cmake</build_type>
    <rclcpp_components/>
  </export>
</package>
