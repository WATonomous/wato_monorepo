<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>perception_bringup</name>
  <version>0.0.0</version>
  <description>Bringup Package for Perception</description>
  <maintainer email="hello@watonomous.ca">WATonomous</maintainer>
  <license>Apache 2.0</license>

  <!-- To enable Shared Memory IPC -->
  <depend>rmw_zenoh_cpp</depend>
  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>rclcpp_lifecycle</depend>
  <depend>sensor_msgs</depend>
  <depend>cv_bridge</depend>
  <depend>vision_msgs</depend>
  <depend>visualization_msgs</depend>
  <depend>deep_core</depend>
  <depend>deep_msgs</depend>
  <depend>deep_ort_backend_plugin</depend>
  <depend>deep_ort_gpu_backend_plugin</depend>
  <depend>onnxruntime_vendor</depend>
  <depend>onnxruntime_gpu_vendor</depend>
  <depend>libopencv-dev</depend>
  <depend>rcl_interfaces</depend>
  <buildtool_depend>ament_cmake</buildtool_depend>

  <exec_depend>patchworkpp</exec_depend>
  <exec_depend>wato_lifecycle_manager</exec_depend>
  <exec_depend>deep_object_detection</exec_depend>
  <exec_depend>camera_sync</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
