<?xml version="1.0"?>
<package format="3">
  <name>patchworkpp</name>
  <version>0.1.0</version>
  <description>Patchwork++ ROS 2 Wrapper</description>
  <maintainer email="b69zheng@uwaterloo.ca">Brian Zheng</maintainer>
  <license>GPL-3.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <!-- Eigen for types used in public headers -->
  <build_depend>eigen3_cmake_module</build_depend>
  <depend>Eigen3</depend>

  <depend>rcutils</depend>
  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>geometry_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>std_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>

  <exec_depend>ros2launch</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
