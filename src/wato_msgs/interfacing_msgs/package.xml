<?xml version="1.0"?>
<package format="3">
  <name>interfacing_msgs</name>
  <version>0.0.0</version>
  <description>ROS2 interfacing messages</description>

  <maintainer email="hello@watonomous.ca">WATonomous</maintainer>
  <license>Apache 2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <depend>std_msgs</depend>

  <!-- From here https://www.theconstructsim.com/ros2-tutorials-7-how-to-create-a-ros2-custom-message-new/ -->
  <build_depend>rosidl_default_generators</build_depend>
  <exec_depend>rosidl_default_runtime</exec_depend>

  <member_of_group>rosidl_interface_packages</member_of_group>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
