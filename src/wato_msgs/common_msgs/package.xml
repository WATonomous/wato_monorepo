<?xml version="1.0"?>
<package format="3">
  <name>common_msgs</name>
  <version>0.0.0</version>
  <description>ROS2 common messages</description>

  <maintainer email="s36gong@watonomous.ca">Steven Gong</maintainer>
  <license>TODO</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <depend>std_msgs</depend>
  <depend>geometry_msgs</depend>

  <!-- From here https://www.theconstructsim.com/ros2-tutorials-7-how-to-create-a-ros2-custom-message-new/ -->
  <depend>builtin_interfaces</depend>
  <depend>rosidl_default_generators</depend>
  <exec_depend>rosidl_default_runtime</exec_depend>

  <member_of_group>rosidl_interface_packages</member_of_group>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
