<?xml version="1.0"?>
<package format="3">
  <name>sample_msgs</name>
  <version>0.0.0</version>
  <description>Sample ROS2 messages</description>

  <maintainer email="cmahalingam@watonomous.ca">Cheran Mahalingam</maintainer>
  <license>TODO</license>

  <!--
    - buildtool_depend declares a ROS2 dependency that is executed during the build process
    - exec_depend declares a ROS2 dependency needed at execution time
    - depend is the equivalent of having <build_depend>, <build_export_depend>, and <exec_depend>
    - export 
    - member_of_group
    More information on dependency tags can be found at https://ros.org/reps/rep-0149.html#dependency-tags
  -->
  <buildtool_depend>ament_cmake</buildtool_depend>
  <depend>std_msgs</depend>

  <build_depend>rosidl_default_generators</build_depend>
  <exec_depend>rosidl_default_runtime</exec_depend>
  <member_of_group>rosidl_interface_packages</member_of_group>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
