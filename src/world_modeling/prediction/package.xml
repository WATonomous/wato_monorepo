<?xml version="1.0"?>
<package format="3">
  <name>prediction</name>
  <version>0.0.1</version>
  <description>Multi-modal trajectory prediction system with map-aware intent inference</description>

  <maintainer email="yashjoshi@watonomous.ca">Yash Joshi</maintainer>
  <license>Apache-2.0</license>

  <!--https://www.ros.org/reps/rep-0149.html#dependency-tags-->
  <!--ROS 2 and Message Dependencies-->
  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>std_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>vision_msgs</depend>
  <depend>eigen</depend>

  <!-- TODO: Add when message types are defined -->
  <!-- <depend>world_modeling_msgs</depend> -->

  <test_depend>ament_cmake_gtest</test_depend>

  <!--https://www.ros.org/reps/rep-0149.html#export-->
  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
