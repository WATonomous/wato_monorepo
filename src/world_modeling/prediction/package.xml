<?xml version="1.0"?>
<package format="3">
  <name>prediction</name>
  <version>0.0.1</version>
  <description>Generates Multi-modal trajectory predictions from tracked objects as WorldObjects</description>

  <maintainer email="r6lei@watonomous.ca">Ryan Lei</maintainer>
  <license>Apache-2.0</license>

  <!--https://www.ros.org/reps/rep-0149.html#dependency-tags-->
  <!--ROS 2 and Message Dependencies-->
  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_lifecycle</depend>
  <depend>std_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>vision_msgs</depend>
  <depend>world_model_msgs</depend>
  <depend>lanelet_msgs</depend>
  <depend>world_model</depend>
  <depend>lanelet2_core</depend>
  <depend>lanelet2_routing</depend>
  <depend>eigen</depend>

  <test_depend>ament_cmake_gtest</test_depend>

  <!--https://www.ros.org/reps/rep-0149.html#export-->
  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
