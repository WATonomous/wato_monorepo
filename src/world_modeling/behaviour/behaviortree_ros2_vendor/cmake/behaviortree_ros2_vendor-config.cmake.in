# Generated config for behaviortree_ros2_vendor

set(behaviortree_ros2_vendor_PREFIX "@BT_ROS2_VENDOR_INSTALL_PREFIX@")

# You may need to adjust these after the first build if the library name differs.
set(_bt_ros2_lib "${behaviortree_ros2_vendor_PREFIX}/lib/libbehaviortree_ros2.so")

if(NOT EXISTS "${_bt_ros2_lib}")
  # Fallback: static lib or different naming (adjust if needed after first build)
  set(_bt_ros2_lib "${behaviortree_ros2_vendor_PREFIX}/lib/libbehaviortree_ros2.a")
endif()

# Imported target for linking
add_library(behaviortree_ros2_vendor::behaviortree_ros2 UNKNOWN IMPORTED)
set_target_properties(behaviortree_ros2_vendor::behaviortree_ros2 PROPERTIES
  IMPORTED_LOCATION "${_bt_ros2_lib}"
  INTERFACE_INCLUDE_DIRECTORIES "${behaviortree_ros2_vendor_PREFIX}/include"
)

