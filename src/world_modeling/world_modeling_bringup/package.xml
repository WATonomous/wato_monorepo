<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>world_modeling_bringup</name>
  <version>0.0.0</version>
  <description>Bringup Package for World Modeling</description>
  <maintainer email="hello@watonomous.ca">WATonomous</maintainer>
  <license>Apache 2.0</license>

  <!-- To enable Shared Memory IPC -->
  <depend>rmw_zenoh_cpp</depend>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <exec_depend>world_model</exec_depend>
  <exec_depend>simple_prediction</exec_depend>
  <exec_depend>lanelet_markers</exec_depend>
  <exec_depend>world_model_markers</exec_depend>
  <exec_depend>costmap</exec_depend>
  <exec_depend>costmap_markers</exec_depend>
  <exec_depend>behaviour</exec_depend>
  <exec_depend>behaviour_markers</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>