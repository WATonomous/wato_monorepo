<?xml version="1.0"?>
<package format="3">
  <name>localization</name>
  <version>0.0.0</version>
  <description>A ROS2 package for vehicle localization</description>

  <maintainer email="m2farooq@uwaterloo.ca">Muhammad Farooqi</maintainer>
  <license>NA</license>

  <!--https://www.ros.org/reps/rep-0149.html#dependency-tags-->
  <buildtool_depend>ament_cmake</buildtool_depend>
  <depend>std_msgs</depend>
  <depend>rclcpp</depend>
  <depend>nav_msgs</depend>
  <depend>path_planning_msgs</depend>
  <depend>carla_msgs</depend>


  <test_depend>ament_cmake_gtest</test_depend>

  <!--https://www.ros.org/reps/rep-0149.html#export-->
  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
