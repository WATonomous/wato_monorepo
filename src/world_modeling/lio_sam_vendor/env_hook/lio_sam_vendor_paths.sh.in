# Relative prefix in case of relocation
HERE=$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )
PREFIX=$(dirname $(dirname $(dirname $HERE)))

# Fixed path at build time
ament_prepend_unique_value AMENT_PREFIX_PATH "@CMAKE_INSTALL_PREFIX@/opt/lio_sam_vendor"
ament_prepend_unique_value CMAKE_PREFIX_PATH "@CMAKE_INSTALL_PREFIX@/opt/lio_sam_vendor"
ament_prepend_unique_value LD_LIBRARY_PATH "@CMAKE_INSTALL_PREFIX@/opt/lio_sam_vendor/lib"
ament_prepend_unique_value PYTHONPATH "@CMAKE_INSTALL_PREFIX@/opt/lio_sam_vendor/lib/python3/dist-packages"
